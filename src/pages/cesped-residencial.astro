---
import Layout from "../layouts/Layout.astro";
import { Header } from "../components/Header";
import { InfoCespedResidencial } from "../components/pages/InfoCespedResidencial";
import { Footer } from "../components/Footer";
import { GaleryCarrusel } from "../components/GaleryCarrusel";
const imagesURL = [
  "/cesped-residencial/uno.webp",
  "/cesped-residencial/dos.webp",
  "/cesped-residencial/tres.webp",
  "/cesped-residencial/cuatro.webp",
  "/cesped-residencial/cinco.webp",
  "/cesped-residencial/seis.webp",
  "/cesped-residencial/siete.webp",
];
const URL =
  "https://wa.me/5212219690933?text=Hola%2C%20me%20gustar%C3%ADa%20cotizar%20una%20instalaci%C3%B3n%20de%20c%C3%A9sped%20sint%C3%A9tico%20residencial.";
---

<Layout
  title="Instalación de Césped Sintético Residencial - DecoStore"
  description="Instalación de césped sintético residencial ideal para jardines, patios o terrazas. Disfruta de un entorno verde todo el año, sin riego ni mantenimiento constante, con apariencia natural y excelente resistencia al desgaste."
  keywords="instalación de césped residencial, césped sintético para jardín, césped artificial para hogar, pasto sin mantenimiento, jardín sin riego, pasto decorativo, instalación de pasto artificial"
  ogImageLink=""
>
  <Header client:only="react" />
  <InfoCespedResidencial client:only="react" classes="px-5 pt-5" />
  <GaleryCarrusel
    client:only="react"
    classes="pt-10 4sm:pt-20 md:pt-32"
    imagesURL={imagesURL}
  />
  <Footer
    client:only="react"
    href={URL}
    classes={"px-5 py-10 4sm:py-20 md:py-32"}
  />
</Layout>
