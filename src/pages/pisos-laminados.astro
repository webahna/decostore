---
import Layout from "../layouts/Layout.astro";
import { Header } from "../components/Header";
import { InfoPisosLaminados } from "../components/pages/InfoPisosLaminados";
import { Footer } from "../components/Footer";
import { GaleryCarrusel } from "../components/GaleryCarrusel";
const imagesURL = [
  "/cesped-residencial/uno.webp",
  "/cesped-residencial/dos.webp",
];
---

<Layout
  title="Instalación de Pisos Laminados - DecoStore"
  description="Instalamos pisos laminados con acabados tipo madera, resistentes al uso diario y fáciles de mantener. Una opción estética y práctica para renovar interiores con rapidez y estilo."
  keywords="instalación de pisos laminados, piso tipo madera, laminado resistente, renovación de interiores, pisos modernos, pisos fáciles de limpiar, instalación de piso decorativo"
>
  <Header client:only="react" />
  <InfoPisosLaminados client:only="react" />
  <GaleryCarrusel
    client:only="react"
    classes="pt-10 4sm:pt-20 md:pt-32"
    imagesURL={imagesURL}
  />
  <Footer client:only="react" classes={"px-5 py-10 4sm:py-20 md:py-32"} />
</Layout>
